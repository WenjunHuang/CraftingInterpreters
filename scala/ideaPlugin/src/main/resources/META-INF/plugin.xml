<idea-plugin>
    <id>com.github.wenjunhuang.lox</id>
    <name>Lox Language Playground</name>
    <vendor>WenjunHuang</vendor>

    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <fileType name="Lox File" implementationClass="com.github.wenjunhuang.lox.ideaplugin.LoxFileType$"
                  fieldName="MODULE$" language="Lox" extensions="lox"/>
        <lang.parserDefinition language="Lox" implementationClass="com.github.wenjunhuang.lox.ideaplugin.LoxParser"/>

    </extensions>

</idea-plugin>
